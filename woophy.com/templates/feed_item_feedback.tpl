<div class="Feedback Post clearfix{alt}">
	<div class="Container clearfix">
		<a class="Thumb sprite ImageContainer" href="{item_url}"><img src="{thumb_src}" alt="{city_name}, {country_name}" /></a>
		<div class="PostText">
			<div class="TextTop">
				<h3>Comment by <a class="MapLink" rel="username:{user_name}" href="{root_url}member/{user_name}">{user_name}</a></h3>
				<span class="Comment">{item_title}</span>
			</div>
			<div class="TextBottom">{item_date}</div>
		</div><!-- end PostText -->
	</div>
</div><!-- end Post -->