<div class="FavoritePhoto Post clearfix{alt}">
	<div class="Container clearfix">
		<a class="Thumb sprite ImageContainer" href="{item_url}"><img src="{thumb_src}" alt="{city_name}, {country_name}" /></a>
		<div class="PostText">
			<div class="TextTop">
				<h3><a class="MapLink" rel="username:{user_name}" href="{root_url}member/{user_name}">{user_name}</a> likes your <a href="{item_url}">photo</a> from <a class="MapLink" rel="cityid:{city_id}" href="{root_url}city/{city_name}/{country_name}">{city_name}</a></h3>
			</div>
			<div class="TextBottom">{item_date}</div>
		</div><!-- end PostText -->
	</div>
</div><!-- end Post -->