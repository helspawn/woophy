<div class="User Post clearfix{alt}">
	<div class="Container clearfix">
		<a class="Thumb sprite" href="{item_url}"><img src="{thumb_src}" alt="{user_name}" /></a>
		<div class="PostText">
			<div class="TextTop"><h3>{label} <a class="MapLink" rel="username:{user_name}" href="{item_url}">{user_name}</a>{location}</h3></div>
			<div class="TextBottom">
				{item_date}
			</div>
		</div><!-- end PostText -->
	</div>
</div><!-- end Post -->