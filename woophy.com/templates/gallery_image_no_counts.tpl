<div class="GalleryPhoto PositionRelative ImageContainer{lightbox}" rel="{root_url}photo/{photo_id}">
	<img class="gallery_image" id="{photo_id}" src="{photo_url}" alt="{alt}" />
	<div class="PhotoMetaContainer js_nodisplay">
		<div class="PhotoMetaBackground PositionAbsolute"></div>
		<div class="PhotoMeta PositionAbsolute">
			<div class="Info"><a class="User" href="{root_url}member/{user_name}">{user_name}</a><span class="Location"><a href="{root_url}city/{city}/{country}">{city}</a>, <a href="{root_url}country/{country}">{country}</a></span></div>
		</div> <!-- end PhotoMeta -->
	</div> <!-- end PhotoMetaContainer -->
</div> <!-- end GalleryPhoto -->
