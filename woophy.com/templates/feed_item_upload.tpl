<div class="Photo Post clearfix{alt}">
	<div class="Container clearfix">
		<a class="Thumb sprite ImageContainer" href="{item_url}"><img src="{thumb_src}" alt="{city_name}, {country_name}" /></a>
		<div class="PostText">
			<div class="TextTop"><h3><a class="MapLink" rel="username:{user_name}" href="{root_url}member/{user_name}">{user_name}</a> uploaded a new photo from {item_title}</h3></div>
			<div class="TextBottom">{item_date}</div>
		</div><!-- end PostText -->
	</div>
</div><!-- end Post -->