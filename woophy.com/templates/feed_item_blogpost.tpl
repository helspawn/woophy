<div class="Blog Post clearfix{alt}">
	<div class="Container clearfix">
		<a class="Thumb sprite ImageContainer" href="{item_url}"><img src="{thumb_src}" alt="{item_title}" /></a>
		<div class="PostText">
			{label}
			<div class="TextTop">
				<h3><a href="{item_url}">{item_title}</a></h3>
			</div>
			<div class="TextBottom">
				{item_date}
			</div>
		</div><!-- end PostText -->
	</div>
</div><!-- end Post -->